{
  "name": "ssr-webpack",
  "version": "1.0.0",
  "description": "Simple React application to test Webpack configuration for Server-Side Rendering",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --color",
    "start": "node dist/server/index.js",
    "watch": "nodemon --watch \"./webpack.*.*\" --exec \"webpack --color --watch\"",
    "dev": "nodemon dist/server/index.js",
    "serve": "webpack serve --mode=development --color --open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "node": "^16.20.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^3.2.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/react-dom": "^16.9.23",
    "@types/react-router": "^3.0.28",
    "@types/webpack": "^4.41.37",
    "@types/webpack-node-externals": "^3.0.4",
    "autoprefixer": "^10.4.16",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
